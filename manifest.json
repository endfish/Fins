{
  "manifest_version": 3,
  "name": "Fins - Geek New Tab",
  "version": "0.0.1",
  "description": "A clean, minimalist new tab extension. Built with Vue 3, Vite, TypeScript, and co-crafted by Gemini.",
  "icons": {
    "16": "public/logo.png",
    "32": "public/logo.png",
    "48": "public/logo.png",
    "128": "public/logo.png"
  },
  "action": {
    "default_title": "Fins"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://www.bing.com/"
  ],
  "background": {
    "type": "module"
  }
}